<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dondo.demo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".DemoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.dondo.demo.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.dondo.demo.examples.MediaSliderDemoActivity" />
        <activity android:name="com.dondo.demo.examples.TextFieldsActivity" />
        <activity android:name="com.dondo.demo.examples.ButtonViewActivity" />
        <activity android:name="com.dondo.demo.examples.ProfilePictureActivity" />
        <activity android:name="com.dondo.demo.examples.QuantityPickerActivity" />
    </application>
</manifest>

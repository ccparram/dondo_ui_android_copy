<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="is_required" format="boolean" />
    <attr name="hint" format="string" />

    <declare-styleable name="MediaSliderView">
        <attr name="double_tap_zoom_enabled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="EditTextField">
        <attr name="android:imeOptions" />
        <attr name="is_required" />
        <attr name="hint" />
        <attr name="placeholder" format="string" />
        <attr name="minLength" format="integer" />
        <attr name="maxLength" format="integer" />
        <attr name="minLines" format="integer" />
        <attr name="maxLines" format="integer" />
        <attr name="textType" format="enum">
            <enum name="text" value="0" />
            <enum name="multiline" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="QuantityPicker">
        <attr name="quantityPickerNextFocusDown" format="reference"/>
    </declare-styleable>
</resources>
